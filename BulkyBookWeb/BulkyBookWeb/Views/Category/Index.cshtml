@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";

}
<h1>Index Category </h1>
<h4>(Views/Category/Index.cshtml)</h4>

<table class= "table table-bordered table-striped" style='width:100%'>
    <thead>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                Display Order
            </th><th>
                ID Number
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td width="40%"><i>@obj.Name</i></td>
                <td width="35%">@obj.DisplayOrder</td>
                <td width="25%">@obj.Id</td>
            </tr>
        }
    </tbody>
</table>